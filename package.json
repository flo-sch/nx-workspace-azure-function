{
  "name": "flosch",
  "description": "Example nx workspace for Azure Function",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "nx build",
    "lint": "nx workspace-lint && nx run-many --all --target lint",
    "nx": "nx",
    "prepare": "husky install",
    "start": "nx serve",
    "test": "nx test"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@nrwl/cli": "12.9.0",
    "@nrwl/eslint-plugin-nx": "12.9.0",
    "@nrwl/jest": "12.9.0",
    "@nrwl/linter": "12.9.0",
    "@nrwl/node": "12.9.0",
    "@nrwl/tao": "12.9.0",
    "@nrwl/web": "12.9.0",
    "@nrwl/workspace": "12.9.0",
    "@types/jest": "27.0.2",
    "@types/node": "14.17.20",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "husky": "^7.0.0",
    "jest": "27.2.4",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "ts-jest": "27.0.5",
    "typescript": "^4.4.3"
  },
  "lint-staged": {
    "*.json": [
      "prettier --write"
    ],
    "{apps,libs,tools}/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
